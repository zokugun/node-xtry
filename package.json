	},
	"bugs": {
		"url": "https://github.com/zokugun/node-xtry/issues"
	},
	"type": "module",
	"main": "lib/index.js",
	"scripts": {
		"commit": "cz",
		"compile": "tsc -p src",
		"lint": "xo",
