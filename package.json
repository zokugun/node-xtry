		"@types/fs-extra": "^11.0.4",
		"@types/mocha": "^10.0.10",
		"@types/node": "^20.14.8",
		"chai": "^5.2.0",
		"commitizen": "^4.3.1",
		"eslint-plugin-chai-friendly": "^1.0.1",
		"fixpack": "^4.0.0",
		"fs-extra": "^11.3.0",
		"globby": "^14.1.0",
		"husky": "^9.1.7",
