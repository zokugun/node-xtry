{
	"name": "@zokugun/xtry",
	"description": "simple try/catch wrapper returning Result",
	"version": "0.6.0",
	"author": {
		"name": "Baptiste Augrain",
		"email": "daiyam@zokugun.org"
	},
	"license": "MIT",
	"homepage": "https://github.com/zokugun/node-xtry",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zokugun/node-xtry.git"
	},
	"bugs": {
		"url": "https://github.com/zokugun/node-xtry/issues"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./lib/types/index.d.ts",
			"import": "./lib/esm/index.js",
			"require": "./lib/cjs/index.js",
			"default": "./lib/esm/index.js"
		},
		"./async": {
			"types": "./lib/types/async.d.ts",
			"import": "./lib/esm/async.js",
			"require": "./lib/cjs/async.js",
			"default": "./lib/esm/async.js"
		},
		"./sync": {
			"types": "./lib/types/sync.d.ts",
			"import": "./lib/esm/sync.js",
			"require": "./lib/cjs/sync.js",
			"default": "./lib/esm/sync.js"
		}
	},
	"main": "lib/cjs/index.js",
	"module": "lib/esm/index.js",
	"types": "lib/types/index.d.ts",
	"typesVersions": {
		"*": {
			"async": [
				"./lib/types/async.d.ts"
			],
			"sync": [
				"./lib/types/sync.d.ts"
			]
		}
	},
	"scripts": {
		"build": "npm run clean && npm run build:cjs && npm run build:esm",
		"build:cjs": "tsc -p src/tsconfig.cjs.json",
		"build:esm": "tsc -p src/tsconfig.json",
		"clean": "rimraf lib .test",
		"commit": "cz",
		"compile": "tsc -p src",
		"lint": "xo",
		"lint:fix": "xo --fix",
		"prepare": "husky; fixpack || true",
		"prepublishOnly": "npm run compile",
		"release": "release-it",
		"test": "vitest run",
		"watch:source": "tsc-watch -p src",
		"watch:test": "vitest"
	},
	"dependencies": {},
	"devDependencies": {
		"@commitlint/cli": "^19.7.1",
		"@commitlint/config-conventional": "^19.7.1",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.14.8",
		"commitizen": "^4.3.1",
		"fixpack": "^4.0.0",
		"fs-extra": "^11.3.3",
		"globby": "^16.1.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.4",
		"release-it": "^18.1.2",
		"tsc-watch": "^6.3.0",
		"typescript": "^5.7.3",
		"vitest": "^4.0.16",
		"xo": "0.60.0"
	},
	"keywords": [
		"catch",
		"error",
		"result",
		"try"
	]
}
